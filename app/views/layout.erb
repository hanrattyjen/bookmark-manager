<!DOCTYPE html>
<html>
  <head>
    <title>Bookmark Manager</title>
  </head>

<body>
  <%= partial :welcome %>
  <%= partial :flash %>
  <% if !current_user %>
    <form action="/users/sign_up">
      <input type="submit" value="Sign Up">
    </form>
    <form action="/sessions/sign_in">
      <input type="submit" value="Sign In">
    </form>
  <% end %>
  <%= yield %>
</body>
</html>
